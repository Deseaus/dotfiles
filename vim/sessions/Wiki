let SessionLoad = 1
if &cp | set nocp | endif
let s:so_save = &so | let s:siso_save = &siso | set so=0 siso=0
let v:this_session=expand("<sfile>:p")
silent only
cd ~/Documents/NLP/GF/lib/src/style
if expand('%') == '' && !&modified && line('$') <= 1 && getline(1) == ''
  let s:wipebuf = bufnr('%')
endif
set shortmess=aoO
badd +4 RuleAll.gf
badd +1 RuleApproved.gf
badd +1 RuleHint.gf
badd +1 RuleToHint.gf
badd +4 RuleToRGL.gf
badd +5 StyleCat.gf
badd +4 StyleCatAbs.gf
badd +16 StyleRules.gf
badd +1 Wiki.gf
badd +5 WikiEdit.gf
badd +1 WikiHint.gf
badd +1 WikiMaster.gf
badd +5 WikiParse.gf
badd +27 ManualDeEstilo.gf
badd +1 Seasons.gf
badd +3 Season.gf
badd +1 SeasonSpa.gf
badd +5 PunctuationAbs.gf
badd +5 Punctuation.gf
badd +20 Typography.gf
badd +1 List.gf
badd +15 ListSpa.gf
argglobal
silent! argdel *
argadd RuleAll.gf
argadd RuleApproved.gf
argadd RuleHint.gf
argadd RuleToHint.gf
argadd RuleToRGL.gf
argadd StyleCat.gf
argadd StyleCatAbs.gf
argadd StyleRules.gf
argadd Wiki.gf
argadd WikiEdit.gf
argadd WikiHint.gf
argadd WikiMaster.gf
argadd WikiParse.gf
edit StyleRules.gf
set splitbelow splitright
set nosplitbelow
set nosplitright
wincmd t
set winheight=1 winwidth=1
argglobal
edit StyleRules.gf
setlocal fdm=manual
setlocal fde=0
setlocal fmr={{{,}}}
setlocal fdi=#
setlocal fdl=0
setlocal fml=1
setlocal fdn=20
setlocal fen
silent! normal! zE
let s:l = 12 - ((11 * winheight(0) + 28) / 57)
if s:l < 1 | let s:l = 1 | endif
exe s:l
normal! zt
12
normal! 0
lcd ~/Documents/NLP/GF/lib/src/style
tabnext 1
if exists('s:wipebuf')
  silent exe 'bwipe ' . s:wipebuf
endif
unlet! s:wipebuf
set winheight=1 winwidth=20 shortmess=filnxtToO
let s:sx = expand("<sfile>:p:r")."x.vim"
if file_readable(s:sx)
  exe "source " . fnameescape(s:sx)
endif
let &so = s:so_save | let &siso = s:siso_save
doautoall SessionLoadPost
unlet SessionLoad
